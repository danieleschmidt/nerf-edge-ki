# NeRF Edge Kit Production Environment Configuration
# Security Warning: Never commit actual secrets to version control
# Replace all REPLACE_WITH_* placeholders with actual secure values

# Application Configuration
NODE_ENV=production
PORT=8080
HTTPS_PORT=8443
ENABLE_HTTPS=true

# GPU and Performance Configuration
GPU_ACCELERATION=enabled
WEBGPU_BACKEND=vulkan
VULKAN_SDK_PATH=/usr
MEMORY_POOL_SIZE=4096
MAX_CONCURRENT_RENDERS=12
WORKER_THREADS=8
TARGET_FPS=90
MAX_MEMORY_MB=8192

# Advanced Feature Configuration
FOVEATED_RENDERING=enabled
NEURAL_COMPRESSION=enabled
SPATIAL_SYNC_ENABLED=true
QUANTUM_OPTIMIZATION=enabled
PREDICTIVE_CACHING=enabled
MULTI_USER_SUPPORT=enabled
ENABLE_NEURAL_ACCELERATION=true
ENABLE_HYPER_DIMENSIONAL_RENDERING=true

# Security Configuration
SECURITY_SCANNING=enabled
COMPLIANCE_VALIDATION=enabled
JWT_SECRET=REPLACE_WITH_SECURE_JWT_SECRET_64_CHARS_MINIMUM
ENCRYPTION_KEY=REPLACE_WITH_32_CHARACTER_ENCRYPTION_KEY
CORS_ORIGIN=https://your-domain.com
CSRF_SECRET=REPLACE_WITH_SECURE_CSRF_SECRET
RATE_LIMIT_MAX=1000
RATE_LIMIT_WINDOW=900000
ENABLE_CSP=true
ALLOWED_ORIGINS=https://your-domain.com,https://www.your-domain.com

# Database Configuration
DATABASE_URL=postgresql://user:password@postgres:5432/nerfkit
DATABASE_POOL_SIZE=20
DATABASE_TIMEOUT=30000

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_PASSWORD=REPLACE_WITH_REDIS_PASSWORD
REDIS_DB=0
REDIS_MAX_CONNECTIONS=20

# Monitoring and Telemetry
ENABLE_TELEMETRY=true
PROMETHEUS_METRICS_ENABLED=true
LOG_LEVEL=info
SENTRY_DSN=REPLACE_WITH_SENTRY_DSN
HEALTH_CHECK_INTERVAL=30000
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_ERROR_REPORTING=true
TELEMETRY_ENDPOINT=https://api.your-domain.com/telemetry
ERROR_REPORTING_ENDPOINT=https://api.your-domain.com/errors

# Spatial Computing Configuration
SPATIAL_RESOLUTION=4096
SPATIAL_FRAME_RATE=90
SPATIAL_LATENCY_TARGET=4.2
SPATIAL_POWER_TARGET=8.0
EYE_TRACKING_ENABLED=true
HAND_TRACKING_ENABLED=true
VISION_PRO_OPTIMIZATIONS=true
IPHONE_OPTIMIZATIONS=true

# Neural Network Configuration
MODEL_CACHE_SIZE=1024
MODEL_PRECISION=float16
BATCH_SIZE=16
INFERENCE_TIMEOUT=100

# Compliance and Globalization
GDPR_ENABLED=true
CCPA_ENABLED=true
HIPAA_ENABLED=true
DEFAULT_LOCALE=en-US
SUPPORTED_LOCALES=en-US,es-ES,fr-FR,de-DE,zh-CN,ja-JP,ko-KR,pt-BR

# SSL/TLS Configuration
SSL_CERT_PATH=/app/ssl/cert.pem
SSL_KEY_PATH=/app/ssl/private.key
SSL_CA_PATH=/app/ssl/ca.pem
TLS_VERSION=1.3

# Cloud and Scaling Configuration
CLOUD_PROVIDER=aws
AWS_REGION=us-west-2
S3_BUCKET=nerf-edge-kit-models
CDN_URL=https://cdn.your-domain.com
AUTO_SCALING_ENABLED=true
MIN_INSTANCES=3
MAX_INSTANCES=20
ASSET_CACHE_TTL=86400
ENABLE_ASSET_COMPRESSION=true

# Build and Optimization Settings
BUILD_OPTIMIZATION_LEVEL=maximum
ENABLE_MINIFICATION=true
ENABLE_TREE_SHAKING=true
BUNDLE_ANALYZER=false
ENABLE_COMPRESSION=true
ENABLE_CACHING=true
CACHE_SIZE_MB=512
ENABLE_QUANTIZATION=true

# WebGPU Settings
ENABLE_WEBGPU=true
WEBGPU_VALIDATION=false
WEBGPU_DEBUG=false

# Feature Flags
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_RESEARCH_MODE=false
ENABLE_DEBUG_OVERLAY=false

# Backup and Recovery
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
DISASTER_RECOVERY_ENABLED=true

# Third-party Integrations
GITHUB_WEBHOOK_SECRET=REPLACE_WITH_GITHUB_SECRET
SLACK_WEBHOOK_URL=REPLACE_WITH_SLACK_WEBHOOK
DATADOG_API_KEY=REPLACE_WITH_DATADOG_KEY

# Development and Debugging (disabled in production)
DEBUG=false
VERBOSE_LOGGING=false
PROFILING_ENABLED=false
MEMORY_PROFILING=false