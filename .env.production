# Production Environment Configuration for NeRF Edge Kit

# Node Environment
NODE_ENV=production

# Performance Settings
TARGET_FPS=90
MAX_MEMORY_MB=1024
ENABLE_FOVEATED_RENDERING=true
ENABLE_NEURAL_ACCELERATION=true

# WebGPU Settings
ENABLE_WEBGPU=true
WEBGPU_VALIDATION=false
WEBGPU_DEBUG=false

# Optimization Settings
ENABLE_COMPRESSION=true
ENABLE_CACHING=true
CACHE_SIZE_MB=512
ENABLE_QUANTIZATION=true

# Monitoring and Telemetry
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_ERROR_REPORTING=true
TELEMETRY_ENDPOINT=https://api.your-domain.com/telemetry
ERROR_REPORTING_ENDPOINT=https://api.your-domain.com/errors

# Security Settings
ENABLE_CSP=true
ALLOWED_ORIGINS=https://your-domain.com,https://www.your-domain.com
API_RATE_LIMIT=1000

# Deployment Settings
BUILD_OPTIMIZATION_LEVEL=maximum
ENABLE_MINIFICATION=true
ENABLE_TREE_SHAKING=true
BUNDLE_ANALYZER=false

# Feature Flags
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_QUANTUM_OPTIMIZATION=true
ENABLE_RESEARCH_MODE=false
ENABLE_DEBUG_OVERLAY=false

# Device-Specific Optimizations
VISION_PRO_OPTIMIZATIONS=true
IPHONE_OPTIMIZATIONS=true
ANDROID_OPTIMIZATIONS=false

# CDN and Asset Delivery
CDN_BASE_URL=https://cdn.your-domain.com
ASSET_CACHE_TTL=86400
ENABLE_ASSET_COMPRESSION=true