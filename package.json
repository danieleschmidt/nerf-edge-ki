{
  "name": "nerf-edge-kit",
  "version": "1.0.0",
  "description": "Real-time Neural Radiance Field SDK for spatial computing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && webpack --mode production",
    "dev": "webpack serve --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:perf": "jest --config jest-performance.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage .nyc_output",
    "security-check": "npm audit && npm outdated",
    "prepare": "husky install",
    "prebuild": "npm run clean && npm run lint && npm run typecheck",
    "postinstall": "npm run prepare",
    "start": "npm run dev"
  },
  "keywords": [
    "nerf",
    "neural-radiance-fields",
    "3d-rendering",
    "webgpu",
    "spatial-computing",
    "vision-pro",
    "real-time"
  ],
  "author": "Daniel Schmidt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/nerf-edge-kit.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  }
}